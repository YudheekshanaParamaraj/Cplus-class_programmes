#include <iostream>
using namespace std;
int main(){
    cout<<"EXPERIMENT 3 : FIND PRIME NUMBER : YUDHEEKSHANA "<<endl;
    int a,i;
    cout<<"Enter a number:";
    cin>>a;
    for(i=2;i<=a;i++){
        if(a%i==0){
            cout<<a<<" is not a prime number";
            break;
        }
        else{
            cout<<a<<" is a prime number";
            break;
        }
    }
}
